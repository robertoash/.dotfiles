alias snowflake='cd ~/dev/work/dbt-models/snowflake'

# Hook direnv
eval "$(direnv hook zsh)"

# Hook asdf
. /opt/asdf-vm/asdf.sh

## Google Cloud SDK
# The next line updates PATH for the Google Cloud SDK.
if [ -f '/home/rash/google-cloud-sdk/path.zsh.inc' ]; then . '/home/rash/google-cloud-sdk/path.zsh.inc'; fi
# The next line enables shell command completion for gcloud.
if [ -f '/home/rash/google-cloud-sdk/completion.zsh.inc' ]; then . '/home/rash/google-cloud-sdk/completion.zsh.inc'; fi

# Modify LD_LIBRARY_PATH for fabrica
export LD_LIBRARY_PATH=/usr/local/lib:$LD_LIBRARY_PATH