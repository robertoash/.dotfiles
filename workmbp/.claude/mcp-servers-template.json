{
  "atlassian": {
    "type": "stdio",
    "command": "uvx",
    "args": ["mcp-atlassian"],
    "env": {
      "JIRA_URL": "{{JIRA_URL}}",
      "JIRA_USERNAME": "{{JIRA_USERNAME}}",
      "JIRA_API_TOKEN": "{{JIRA_API_TOKEN}}",
      "JIRA_PROJECTS_FILTER": "AN2",
      "CONFLUENCE_SPACES_FILTER": "AN"
    }
  }
}
