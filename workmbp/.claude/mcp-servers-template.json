{
  "dbt": {
    "type": "stdio",
    "command": "uvx",
    "args": ["dbt-mcp"],
    "env": {
      "DBT_PROJECT_DIR": "/Users/rash/dev/dbt/dbt_production",
      "DBT_PATH": "/opt/homebrew/bin/dbt"
    }
  },
  "atlassian": {
    "type": "stdio",
    "command": "uvx",
    "args": ["mcp-atlassian"],
    "env": {
      "JIRA_URL": "{{JIRA_URL}}",
      "JIRA_USERNAME": "{{JIRA_USERNAME}}",
      "JIRA_API_TOKEN": "{{JIRA_API_TOKEN}}",
      "JIRA_PROJECTS_FILTER": "AN2",
      "CONFLUENCE_SPACES_FILTER": "AN"
    }
  },
  "claude-flow": {
    "type": "stdio",
    "command": "npx",
    "args": ["claude-flow@v3alpha", "mcp", "start"]
  }
}
