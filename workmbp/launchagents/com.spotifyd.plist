<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.spotifyd</string>

    <key>ProgramArguments</key>
    <array>
        <string>/opt/homebrew/bin/spotifyd</string>
        <string>--no-daemon</string>
        <string>--config-path</string>
        <string>/Users/rash/.config/spotifyd/spotifyd.conf</string>
    </array>

    <key>RunAtLoad</key>
    <true/>

    <key>KeepAlive</key>
    <dict>
        <key>SuccessfulExit</key>
        <false/>
    </dict>

    <key>StandardOutPath</key>
    <string>/tmp/spotifyd.out.log</string>

    <key>StandardErrorPath</key>
    <string>/tmp/spotifyd.err.log</string>

    <key>WorkingDirectory</key>
    <string>/Users/rash</string>

    <key>ThrottleInterval</key>
    <integer>10</integer>
</dict>
</plist>
