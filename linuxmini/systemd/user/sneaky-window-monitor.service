[Unit]
Description=Sneaky Window Monitor - Keeps sneaky-tagged windows away from active window
PartOf=graphical-session.target
After=graphical-session.target

[Service]
Type=simple
ExecStart=/usr/bin/python3 -u /home/rash/.dotfiles/linuxmini/scripts/sneaky_window_monitor.py
Restart=on-failure
RestartSec=5
Environment=PYTHONUNBUFFERED=1

[Install]
WantedBy=default.target
